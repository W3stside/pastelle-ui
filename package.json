    "netlify:dev": "netlify dev",
    "netlify:help": "netlify --help",
    "start": "yarn dlx serve@latest dist",
    "serve": "yarn dlx serve dist -l 666",
    "build:analyze": "yarn build && yarn analyze",
    "build:serve": "DISABLE_ESLINT_PLUGIN='true' yarn build && yarn start",
    "test": "yarn node $(yarn bin jest)",
    "test:debug": "vite --inspect-brk test --runInBand --no-cache",
    "dedup": "yarn dlx yarn-deduplicate",
